
<div>
<h1>Iniciar sesión</h1>
<form id="login-form" class="login-form">
    <div class="form-group">
        <label for="email">Usuario:</label>
        <input type="email" id="email" name="email" placeholder="Ingrese su usuario" required>
    </div>
    <div class="form-group">
        <label for="password">Contraseña:</label>
        <input type="password" id="password" name="password" placeholder="Ingrese su contraseña" required>
    </div>
    <button id="login" type="submit">Ingresar</button>
    <a href="/signup" id="registrarse">Registrarse</a>
</form>
<div>
    <a href="/api/sessions/github">
        <button class="github-button">Entrar con github</button>
    </a>
</div>
</div>
<script src="js/login.js"></script>